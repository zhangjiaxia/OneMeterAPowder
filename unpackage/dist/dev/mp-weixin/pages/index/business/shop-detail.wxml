<view><navigation-bar vue-id="eb5a5b52-1" navigationBarStyle="{{navigationBarStyle}}" bind:__l="__l"></navigation-bar><view class="container"><swiper class="swiper" circular autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{goodsDetail.detailImgUrlList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image class="banner-img" src="{{item}}"></image></swiper-item></block></swiper><view class="shop-message"><view class="uni-flex uni-row vertical"><view class="uni-flex rest shop-price"><block wx:if="{{isVip==1}}"><text style="margin-right:18rpx;">{{"￥"+goodsDetail.vipPrice[0]}}</text></block><text style="{{'text-decoration:'+(isVip==1?'line-through':'none')+';'+('color:'+(isVip==1?'#999999':'#0071CF')+';')}}">{{'￥'+(goodsDetail.retailPrice[0]||'0.00')+''}}</text></view><view class="uni-flex"><text style="color:#0071CF;font-size:24rpx;">{{'+'+$root.g0/100+'积分'}}</text></view></view><view class="shop-title">{{goodsDetail.name||''}}</view><view class="shop-postage">全国包邮</view></view><view class="tab-list"><view class="tab-item"><view class="icon-gou1 tab-icon"></view><text>正品保证</text></view><view class="tab-item"><view class="icon-gou1 tab-icon"></view><text>全场包邮</text></view><view class="tab-item"><view class="icon-gou1 tab-icon"></view><text>全场钜惠</text></view><view class="tab-item"><view class="icon-gou1 tab-icon"></view><text>售后无忧</text></view></view><view class="detail"><view class="rich-text"><rich-text nodes="{{goodsDetail.detailInfo}}"></rich-text></view></view><view class="goodsdeclaration"><image style="width:100%;" src="/static/goods-desc.png" mode="aspectFit"></image></view><view class="detail-bottom"><view class="uni-flex vertical detail-bottom-left"><view data-event-opts="{{[['tap',[['startShare']]]]}}" class="detail-bottom-item" bindtap="__e"><image class="share-icon" src="/static/share-icon.png"></image><view>分享</view></view><view data-event-opts="{{[['tap',[['shoppingPage',['$event']]]]]}}" class="detail-bottom-item" bindtap="__e"><image class="share-icon" src="/static/shopping-icon.png"></image><view>购物车</view></view></view><auth-page vue-id="eb5a5b52-2" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['showPanel',[true]]]]]}}" class="shop-btn" bindtap="__e">加入购物车</view></auth-page><auth-page vue-id="eb5a5b52-3" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['showPanel',[false]]]]]}}" class="pay-btn" bindtap="__e">立即购买</view></auth-page></view></view><block wx:if="{{confirmModal}}"><view data-event-opts="{{[['touchmove',[['touch',['$event']]]]]}}" class="confirm-modal" catchtouchmove="__e"><view class="confirm-modal-content"><view class="modal-shop-message"><image class="modal-shop-img" src="{{goodsDetail.mainImgUrl}}"></image><view class="modal-shop-content"><view>{{goodsDetail.name}}</view><view class="shop-price">{{"￥"+(goodsDetail.retailPrice[0]||0)}}</view><view class="amount">{{"库存 "+(inventoryCount||0)+" 件"}}</view></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="icon-cha- modal-shop-close" bindtap="__e"></view></view><scroll-view style="max-height:400rpx;" scroll-y="true"><block wx:for="{{skuPropList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="spec-content"><view>{{index}}</view><view class="spec-list"><block wx:for="{{item}}" wx:for-item="subItem" wx:for-index="i" wx:key="i"><text data-event-opts="{{[['tap',[['chooseProps',[index,i]]]]]}}" class="{{['spec-item',(i==propsCheck[index])?'spec-active':'']}}" bindtap="__e">{{subItem+''}}</text></block></view></view></block></scroll-view><view class="shop-item-bottom"><text>购买数量</text><view class="shop-num"><text data-event-opts="{{[['tap',[['reduceNum',['$event']]]]]}}" class="reduct-btn" bindtap="__e">-</text><text class="num">{{shopNum}}</text><text data-event-opts="{{[['tap',[['addNum',['$event']]]]]}}" class="add-btn" bindtap="__e">+</text></view></view><view data-event-opts="{{[['tap',[['handleConfirm',['$event']]]]]}}" class="modal-bottom" bindtap="__e"><text>确认支付</text></view></view></view></block><block wx:if="{{shareModal}}"><view data-event-opts="{{[['touchmove',[['touch',['$event']]]]]}}" class="uni-flex uni-column content share-panel" style="{{'top:'+(panelTop)+';'}}" catchtouchmove="__e"><canvas class="canvas-poster" style="width:650rpx;height:900rpx;" id="canvasPoster" canvas-id="canvasPoster"></canvas><block wx:if="{{poster}}"><image class="poster" style="width:650rpx;height:900rpx;position:absolute;top:0;" src="{{poster}}" alt data-event-opts="{{[['tap',[['previewImg',['$event']]]]]}}" bindtap="__e"></image></block><view data-event-opts="{{[['tap',[['saveImg',['$event']]]]]}}" class="uni-flex content share-btn" style="{{'visibility:'+(poster?'visible':'hidden')+';'}}" bindtap="__e">分享好友</view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="share-img" bindtap="__e"><image class="share-img" src="/static/close.png"></image></view></view></block><view data-event-opts="{{[['tap',[['e2',['$event']]]],['touchmove',[['touch',['$event']]]]]}}" hidden="{{!(confirmModal||shareModal)}}" class="mask" bindtap="__e" catchtouchmove="__e"></view></view>