
.container{
	background-color: #ffffff;
	padding-top: 50px;
}
.title-contents{
  width: 100%;
	position: fixed;
	top: 0;
}
.titles{  
  height: 50px;
  color: #ffffff;
  background: #0070CF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0rpx 30rpx;
}
.search-input{
	height: 60rpx;
	background-color: #ffffff;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	margin-left: 30rpx;
	margin-right: 30rpx;
	border-radius: 8rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 10rpx 20rpx;
	box-sizing: border-box;
}
.search-icon{
	width: 30rpx;
	height: 30rpx;
	margin-right: 10rpx;
}
.search-input input{
	height: 100%;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	color: #333333;
}
.return-icon{
	width: 20rpx;
	height: 36rpx;
}
.search-text{
	font-size: 36rpx;
}
input{
  caret-color: #FF585C;
}
.search-content{
	padding: 0rpx 30rpx;
	margin-top: 40rpx;
}
.search-title{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin-bottom: 30rpx;
	font-size: 32rpx;
}
.record-list{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.record-item{
	display: inline-block;
	padding: 0rpx 30rpx;
	height: 53rpx;
	line-height: 53rpx;
	background-color: #f4f4f4;
	text-align: center;
	color: #666666;
	font-size: 26rpx;
	border-radius: 26rpx;
	margin-right: 30rpx;
	margin-bottom: 30rpx;
}
.delete-icon{
	width: 32rpx;
	height: 32rpx;
}

