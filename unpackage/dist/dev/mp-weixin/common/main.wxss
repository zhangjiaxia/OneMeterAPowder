/*缺少这个配置，图标显示有问题*/
text[class*="icon-"],
view[class*="icon-"],
button[class*="icon-"] {
    font-family: "iconfont";
    font-size: inherit;
    font-style: normal;
	display: inline;
}

/*每个页面公共css */
page{
	height: 100%;
	font-size: 30rpx;
	color: #333333;
	overflow: unset;
}

/*导航栏吸顶效果,由于封装成组件后会多一层view，导致苹果手机不吸顶*/
.bar-sticky {
	position: sticky;
	position: -webkit-sticky;
	top: 0;
	z-index: 101;
}
.container{
  min-height: 100%;
  background: #EEEEEE;
  position: relative;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.empty-text{
  text-align: center;
  font-size: 26rpx;
  color: #999999;
  margin: 50rpx;
}
.common-btn{
  width: 100%;
  height: 39px;
  line-height: 39px;
  text-align: center;
  font-size: 32rpx;
  background: #4C7DE3;
  border-radius: 10rpx;
  color: #ffffff;
}

