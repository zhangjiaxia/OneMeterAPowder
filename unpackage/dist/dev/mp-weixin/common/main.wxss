














/*每个页面公共css */
page{
	height: 100%;
	font-size: 30rpx;
	color: #333333;
}
.container{
  min-height: 100%;
  background: #EEEEEE;
  position: relative;
  box-sizing: border-box;
}
.empty-text{
  text-align: center;
  font-size: 26rpx;
  color: #999999;
  padding-top: 50rpx;
}
.common-btn{
  width: 100%;
  height: 39px;
  line-height: 39px;
  text-align: center;
  font-size: 32rpx;
  background: #4C7DE3;
  border-radius: 10rpx;
  color: #ffffff;
}

